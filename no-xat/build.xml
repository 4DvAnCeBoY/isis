<?xml version="1.0"?>
<!DOCTYPE project [
	<!ENTITY properties SYSTEM "file:../build-templates/properties.xml">
	<!ENTITY project-build SYSTEM "file:../build-templates/project-build.xml">
]>

<project name="Naked Object Cache Object Store" default="all" basedir=".">
	<description>Prevalyer style object store that journals all changes to the objects, and captures a snapshot
		at shutdown.</description>

	&properties;

	<property name="component" value="no-xat"/>
	
	<!-- classpath -->
	<path id="libs.internal">
		<pathelement location="../no-core/build/classes/src"/>
		<pathelement location="../no-core/build/classes/test"/>
	</path>

	&project-build;	
</project>


