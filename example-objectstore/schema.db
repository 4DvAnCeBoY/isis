create table MOVIE (
	PKid				INT					NOT NULL,
	name				VARCHAR(50) 	NOT NULL,
	directorFK		INT
);

create table PERSON (
	PKid				INT,
	name				VARCHAR(50) 	NOT NULL,
	dob				DATE
);

create table ROLE (
	movieFK			INT 					NOT NULL,
	name				VARCHAR(50) 	NOT NULL,
	personFK		INT
);

